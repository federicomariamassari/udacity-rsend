<?xml version="1.0" encoding="UTF-8"?>

<launch>

  <!-- Make sure a clone of "teleop_twist_keyboard" exists inside catkin/src folder -->

  <!-- Launch with custom velocities to generate a finer 2D grid (https://github.com/ros-teleop/teleop_twist_keyboard) -->

  <node name="teleop_twist_keyboard" pkg="teleop_twist_keyboard" type="teleop_twist_keyboard.py" output="screen" args="_speed:=0.8 _turn:=0.5"/>

</launch>

